package Jorge.utiles {		import gs.TweenLite;	import gs.easing.*;		public class Animaciones {		public function Animaciones() {			// constructor code		}				public static function rebote(quien:*):void{			quien.alpha = 0;			quien.scaleX = quien.scaleY = 0;			TweenLite.to(quien, 0.7, {scaleX:1.35, scaleY:1.35, alpha:1, delay:0.5, ease:Bounce.easeOut, onComplete:reboteBis, onCompleteParams:[quien]});		}		public static function reboteBis(quien:*):void{			TweenLite.to(quien, 0.4, {scaleX:1, scaleY:1, alpha:1, delay:0.3, ease:Back.easeIn});		}	}	}